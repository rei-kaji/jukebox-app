<%- include('../partials/header') %>
<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <h1>Welcome our world</h1>
    <div class="row row-cols-1 row-cols-md-3 g-4">
      <% for(let index= 0; index<songs.length; index++) {%> 
        <div class="col">
            <div class="card h-100">
              <img src="<%= songs[index].imageUrl %>" class="card-img-top" alt="..." />
              <div class="card-header bg-transparent border-success"><%= songs[index].genre %></div>
              <div class="card-body">
                  <h5 class="card-title"> <%= songs[index].title %></h5>
                  <h3></h3>
                  <p class="card-text">
                    <%= songs[index].description %>
                  </p>
                  <a class="nav-link" href="<%= songs[index].link %>">Link</a>
              </div>
            </div>
        </div>
      <% } %>
    </div>
  </body>
</html>
